{
  "name": "begin-game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:04",
    "start:01": "parcel src/01-move-player/index.html",
    "start:02": "parcel src/02-operate-player/index.html",
    "start:03": "parcel src/03-free-movement/index.html",
    "start:04": "parcel src/04-refactor-move/index.html",
    "start:05": "parcel src/05-enemy/index.html",
    "start:06": "parcel src/06-collision/index.html",
    "start:07": "parcel src/07-refactor-death/index.html",
    "start:08": "parcel src/08-module/index.html",
    "clean": "npm-run-all clean:build clean:dist",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "build": "npm-run-all build:01 build:02",
    "build:01": "parcel build src/01-move-player/index.html -d build/01-move-player --public-url '.'",
    "build:02": "parcel build src/02-operate-player/index.html -d build/02-operate-player --public-url '.'",
    "build:03": "parcel build src/03-free-movement/index.html -d build/03-free-movement --public-url '.'",
    "build:04": "parcel build src/04-refactor-move/index.html -d build/04-refactor-move --public-url '.'",
    "build:05": "parcel build src/05-enemy/index.html -d build/05-enemy --public-url '.'",
    "build:06": "parcel build src/06-collision/index.html -d build/06-collision --public-url '.'",
    "build:07": "parcel build src/07-refactor-death/index.html -d build/07-refactor-death --public-url '.'",
    "build:08": "parcel build src/08-module/index.html -d build/08-module --public-url '.'",
    "upload-s3": "s3-upload"
  },
  "author": "y.takeuchi",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.5.1",
    "rimraf": "^2.6.2",
    "s3-upload": "^0.1.0"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0"
  }
}
